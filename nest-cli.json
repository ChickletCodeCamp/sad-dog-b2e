{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "clock": {
      "type": "library",
      "root": "libs/clock",
      "entryFile": "index",
      "sourceRoot": "libs/clock/src",
      "compilerOptions": {
        "tsConfigPath": "libs/clock/tsconfig.lib.json"
      }
    },
    "uuid": {
      "type": "library",
      "root": "libs/uuid",
      "entryFile": "index",
      "sourceRoot": "libs/uuid/src",
      "compilerOptions": {
        "tsConfigPath": "libs/uuid/tsconfig.lib.json"
      }
    }
  }
}